{
  "expo": {
    "name": "Craft Coffee",
    "slug": "craftCoffee",
    "privacy": "public",
    "version": "1.0.1",
    "platforms": ["ios"],
    "githubUrl": "https://github.com/CJSantee/Craft-Coffee",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "asset/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.cjsantee.craftCoffee",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Craft Coffee to use your camera",
        "NSPhotoLibraryAddUsageDescription": "Give Craft Coffee permission to save photos",
        "NSPhotoLibraryUsageDescription": "Give Craft Coffee permission to access your photos"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
