{
  "expo": {
    "name": "Coffee Lab",
    "slug": "coffeeLab",
    "privacy": "public",
    "version": "1.0.0",
    "platforms": ["ios"],
    "githubUrl": "https://github.com/CJSantee/Coffee-Lab",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "asset/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.cjsantee.coffeeLab",
      "buildNumber": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
